{
  "rewrites": [
    {
      "source": "/api/config(.*)",
      "destination": "/api/config"
    },
    {
      "source": "/api/post(.*)",
      "destination": "/api/post"
    },
    {
      "source": "/api/user(.*)",
      "destination": "/api/user"
    },
    {
      "source": "/api(.*)",
      "destination": "/api/index"
    }
  ]
}