{
  "rewrites": [
    {
      "source": "/api/config(.*)",
      "destination": "/api/config"
    },
    {
      "source": "/api/post(.*)",
      "destination": "/api/post"
    },
    {
      "source": "/api/user(.*)",
      "destination": "/api/user"
    },
    {
      "source": "/api(.*)",
      "destination": "/api/index"
    }
  ],
  "headers": [
    {
      "source": "/api(.*)",
      "has": [
        {
          "type": "header",
          "key": "origin",
          "value": "localhost"
        },
        {
          "type": "header",
          "key": "origin",
          "value": "127.0.0.1"
        }
      ],
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}